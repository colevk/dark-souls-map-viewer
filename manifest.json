{
  "manifest_version": 2,
  "name": "Dark Souls Map Viewer",
  "version": "1.0.0",

  "permissions": [
    "system.display",
    "pointerLock"
  ],
  "app": {
    "background": {
      "scripts": ["launch.js"],
      "persistent": false
    }
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "128": "icon128.png",
    "256": "icon256.png"
  }
}